{
  "name": "AE Live plugin",
  "description": "Its a plugin used to add additional functionality to AE/AO to support any accounting product(s).",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "browser_action": {
      "default_title": "Portal Menu",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "https://partner.myob.com/uploads",
    "activeTab"
  ],
  "web_accessible_resources":["js/jquery-3.1.0.min.js"],
  "background": {
    "scripts": ["js/background.js", "js/window.js"]
  },
  "manifest_version": 2
}
